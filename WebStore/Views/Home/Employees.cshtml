@model IEnumerable<WebStore.Models.Employee> 
<!DOCTYPE html>

 <html>
     <head>
         <title>Главная страница</title>
     </head>

     <body>
         <h3>ЗАголовок</h3>
        <p>Привет из первого представления</p>

        <table>
            <thead>
                <tr>                
                    
                <th>Id</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Возраст</th>
                </tr>


            </thead>
            <tbody>
                @foreach(var employee in Model)
            {
                <tr>
                    <td>@employee.Id</td>
                    <td>@employee.LastName</td>
                     <td>@employee.FirstName</td>
                    <td>@employee.Patronymic</td>
                    <td>@employee.Age</td>
                </tr>
            }

            </tbody>

        </table>
     </body>


 </html>